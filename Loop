package loop;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	Scanner scan=new Scanner(System.in);
	System.out.println("Enter the no.");
	int newNo=scan.nextInt();
	for(int i=1;i<=10;i++) {
		System.out.println(newNo+" x "+i+""+" = "+newNo*i);
	}
	

	}

}
